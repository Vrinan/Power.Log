    <link href="/Apps/style/webformstyle.css?v=$AppVersion" rel="stylesheet" type="text/css" />
    <script type="text/javascript" hasvelocity="true">
        var KeyValue = "$Model.data.KeyValue";
        var FormId = "$Model.data.FormId";
        var FormState = "$Model.data.FormState";
        var strEpsProjId = "$!CurrentSession.EpsProjId"
        var KeyWord = "$Model.data.KeyWord";
    </script>

</head>

<body>

    <div class="listfile">
        <div class="show buttonlist" id="Organize_buttonlist">

            <!--<div id="FielView" class="button a" style="border:0px;display:none;width:30%"><a href="#"><i class="fa fa-eye"></i><span>查看</span></a></div>-->
            <div class="button a"><a id="Organize.Save" href="#" onclick="PowerForm.OnBtnSave(this)"><i class="fa fa-save"></i><span>保存</span></a></div>
            <!--<div class="button a"><a href="#"><i class="fa fa-thumbs-up"></i><span>同意</span></a></div>
            <div class="button a"><a href="#"><i class="fa fa-mail-reply"></i><span>驳回</span></a></div>
            <div class="button a"><a href="#"><i class="fa fa-eye"></i><span>流程监控</span></a></div>-->
            <!-- <div class="button" style="border:0px;"><a href="#" id="nav"><i class="fa fa-github-square"></i><span>列表</span></a></div>-->
        </div>
        <!--<div class="hide buttonlist" id="Organize_PayNodes_buttonlist">
            <div class="button a"><a href="#"><i class="fa fa-plus"></i><span>新增</span></a></div>
            <div class="button a"><a href="#"><i class="fa fa-trash"></i><span>删除</span></a></div>
        </div>-->
        <div class=" hide buttonlist" id="file_buttonlist">
            <div id="btnUpload" class="button" style="border: 0px;  width: 30%"><a href="#" id="nav"><i class="fa fa-cloud-upload"></i><span>上传图片</span></a></div>
        </div>
        <div id="tablist">
            <section id="Organize" class=" form show">
                <table border="0" cellspacing="1">
                    <tr>
                        <td width="30%">
                            公司编号
                        </td>
                        <td width="70%">
                            <input id="Organize.Code" name="Code" class="mini-textbox" required="true" requirederrortext="公司编号不能为空" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            公司名称
                        </td>
                        <td>
                            <input id="Organize.Name" name="Name" class="mini-textbox" required="true" requirederrortext="公司名称不能为空" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            公司简称
                        </td>
                        <td>
                            <input id="Organize.ShortName" name="ShortName" class="mini-textbox" required="true" requirederrortext="公司简称不能为空" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            公司法人
                        </td>
                        <td>
                            <input id="Organize.LegalPerson" name="LegalPerson" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            开户银行名称
                        </td>
                        <td>
                            <input id="Organize.BankName" name="BankName" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            开户银行账号
                        </td>
                        <td>
                            <input id="Organize.BankAccount" name="BankAccount" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            开户银行地址
                        </td>
                        <td>
                            <input id="Organize.BankAddress" name="BankAddress" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            主要联系人
                        </td>
                        <td>
                            <input id="Organize.MainLinker" name="MainLinker" textname="MainLinker" class="mini-buttonedit"
                                   emptytext="请选择人员..." onbuttonclick="PowerForm.OnBtnWizard(this)" allowinput="false" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            电话
                        </td>
                        <td>
                            <input id="Organize.Phone" name="Phone" class="mini-textbox" readonly />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Email
                        </td>
                        <td>
                            <input id="Organize.Email" name="Email" class="mini-textbox" readonly />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            地址
                        </td>
                        <td>
                            <input id="Organize.Address" name="Address" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            网址
                        </td>
                        <td>
                            <input id="Organize.WebSite" name="WebSite" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            税号
                        </td>
                        <td>
                            <input id="Organize.TaxCode" name="TaxCode" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            邮编
                        </td>
                        <td>
                            <input id="Organize.PostCode" name="PostCode" class="mini-textbox" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            录入人
                        </td>
                        <td>
                            <input id="Organize.RegHumName" name="RegHumName" class="mini-textbox" readonly="readonly" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            录入时间
                        </td>
                        <td>
                            <input id="Organize.RegDate" name="RegDate" class="mini-datepicker" readonly="readonly" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            备注
                        </td>
                        <td>
                            <input id="Organize.Memo" name="Memo" class="mini-textbox" />
                        </td>
                    </tr>

                </table>
            </section>
            <!--<section class="hide" id="Organize_PayNodes"></section>-->
            <section class="hide" id="file">
                <ul id="FileAttach"></ul>
            </section>
            <ul id="ul1">
                <li class="active"><i class="fa fa-home"></i></br><span>基本信息</span></li>
                <!--<li><i class="fa fa-cogs"></i></br><span>收款节点</span></li>-->
                <li><i class="fa fa-file"></i></br><span id="filecount">附件(0)</span></li>
            </ul>
        </div>
    </div>

    <div class="listdiv" id="listdiv">
        <div class="navbg"></div>
        <div class="navlist">
            <div class="triangle-up"></div>
            <ul>
                <li><a href="javascript:void(0)" onclick="PowerForm.OpenCamera()">打开相机</a></li>
                <li><a href="javascript:void(0)" onclick="PowerForm.OpenGallery()">打开相册</a></li>
            </ul>
        </div>
    </div>


    <script type="text/javascript">
        var PowerForm = new MobileWebForm();

        $(function () {
		PowerForm.Init();

        });
        mini.parse();


    </script>
</body>
</html>
