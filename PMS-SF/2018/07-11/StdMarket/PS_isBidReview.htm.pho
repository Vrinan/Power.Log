                <li><a href="javascript:void(0)" onclick="PowerForm.OpenCamera()">打开相机</a></li>
                <li><a href="javascript:void(0)" onclick="PowerForm.OpenGallery()">打开相册</a></li>
            </ul>
        </div>
    </div>


    <script type="text/javascript">
        var PowerForm = new MobileWebForm();

        $(function () {
		PowerForm.Init();

        });
        mini.parse();


    </script>
